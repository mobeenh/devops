node {
     if (env.BRANCH_NAME == 'master'){
	     stage('build'){
		    echo "This  is master branch in feature"
			}
	}
	    if (env.BRANCH_NAME == 'develop'){
	     stage('build'){
		    echo "This  is develop branch in in feature"
			}
	}
  if (env.BRANCH_NAME == 'feature'){
	     stage('build'){
		    echo "This  is feature branch in feature"
			}
	}
}
