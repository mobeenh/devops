node {
     if (env.BRANCH_NAME == 'master'){
	     stage('build'){
		    echo "This  is master in developes branch"
			}
	}
	    if (env.BRANCH_NAME == 'develop'){
	     stage('build'){
		    echo "This  is develop in developes jenkins branc"
			}
	}
	if (env.BRANCH_NAME == 'feature'){
	     stage('build'){
		    echo "This  is feature branch  in developes jenkins"
			}
	}
}
	
