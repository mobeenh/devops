node {
     if (env.BRANCH_NAME == 'master'){
	     stage('build'){
		    echo "This  is master branch in master jenksin"
			}
	}
	    if (env.BRANCH_NAME == 'develop'){
	     stage('build'){
		    echo "This  is develop in master jenksin"
			}
	}
	if (env.BRANCH_NAME == 'feature'){
	     stage('build'){
		    echo "This  is feature in master jenksin"
			}
	}
}

